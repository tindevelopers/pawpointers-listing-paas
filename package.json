{
  "name": "@tindeveloper/listing-platform-base",
  "version": "1.0.0",
  "private": false,
  "description": "Forkable base template for building listing platforms: real estate, tourism, directories, marketplaces",
  "keywords": [
    "listing-platform",
    "marketplace",
    "real-estate",
    "tourism",
    "directory",
    "nextjs",
    "supabase",
    "multi-tenant",
    "saas",
    "forkable"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tindevelopers/listing-paas-base.git"
  },
  "bugs": {
    "url": "https://github.com/tindevelopers/listing-paas-base/issues"
  },
  "homepage": "https://github.com/tindevelopers/listing-paas-base#readme",
  "author": "Tin Developers",
  "license": "MIT",
  "main": "./src/app/page.tsx",
  "files": [
    "src",
    "public",
    "supabase/migrations",
    "tailwind.config.ts",
    "tsconfig.json",
    "next.config.ts",
    "postcss.config.mjs",
    "README.md",
    "README_LOCAL_SETUP.md",
    "README_STRIPE.md"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.6.1",
  "scripts": {
    "dev": "turbo run dev",
    "dev:admin": "turbo run dev --filter=@tinadmin/admin",
    "dev:portal": "turbo run dev --filter=@tinadmin/portal",
    "dev:api": "turbo run dev --filter=@listing-platform/api-server",
    "dev:local": "supabase start && pnpm dev",
    "dev:localhost": "./scripts/start-localhost.sh",
    "build": "turbo run build",
    "build:packages": "turbo run build --filter='./packages/*'",
    "build:admin": "turbo run build --filter=@tinadmin/admin",
    "build:portal": "turbo run build --filter=@tinadmin/portal",
    "build:api": "turbo run build --filter=@listing-platform/api-server",
    "build:all": "turbo run build",
    "start": "turbo run start",
    "start:admin": "turbo run start --filter=@tinadmin/admin",
    "start:portal": "turbo run start --filter=@tinadmin/portal",
    "start:api": "turbo run start --filter=@listing-platform/api-server",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules .turbo",
    "audit": "pnpm audit --audit-level=moderate",
    "deps:check": "npx npm-check-updates",
    "deps:update": "npx npm-check-updates -u",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "supabase:reset": "supabase db reset",
    "docker:ps": "docker ps | grep supabase",
    "docker:logs": "supabase logs"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.15",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/list": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.15",
    "@heroicons/react": "^2.2.0",
    "@popperjs/core": "^2.11.8",
    "@stripe/react-stripe-js": "^5.4.1",
    "@stripe/stripe-js": "^8.5.3",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.86.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/postcss": "^4.0.9",
    "apexcharts": "^4.3.0",
    "autoprefixer": "^10.4.20",
    "clsx": "^2.1.1",
    "flatpickr": "^4.6.13",
    "next": "^15.4.8",
    "prismjs": "^1.30.0",
    "react": "^19.2.3",
    "react-apexcharts": "^1.7.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.2.3",
    "react-dropzone": "^14.3.5",
    "server-only": "^0.0.1",
    "simplebar-react": "^3.3.0",
    "stripe": "^20.0.0",
    "swiper": "^11.2.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20",
    "@types/prismjs": "^1.26.5",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "15.1.3",
    "postcss": "^8",
    "turbo": "^2.3.3",
    "typescript": "5.9.3",
    "vitest": "^2.1.2"
  }
}
